{
	"manifest_version": 3,
	"name": "Block Websites",
	"description": "Blocks user specified websites after a set amount of time",
	"version": "2.0",
	"author": "Abir Rahi",
	"action": {
		"default_icon": "ban-stop-icon-22.png",
		"default_title": "Block Websites"
	},
	"permissions": ["tabs", "activeTab", "storage"],
	"host_permissions": ["http://127.0.0.1:5000/*"],
	"background": {
		"service_worker": "background.js"
	}
}